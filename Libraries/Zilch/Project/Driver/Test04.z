class UnitTest04
{
  // Test the if/else if/else condition, type inferencing, casting
  [Static]
  function Test04() : Integer
  {
    var i = 42 + CustomMath.Fib(n : 6);
    var j = 3.2;

    if (j > 2.0)
    {
      i = 3;
    }
    else
    {
      i = 6;
    }

    if (j as Integer < i + CustomMath.Factorial(n : 4))
    {
      i += 1;
    }
    else if (i * i > CustomMath.Factorial(n : 3))
    {
      i += 2;
    }
    else
    {
      i += 3;
    }

    return i;
  }
}
