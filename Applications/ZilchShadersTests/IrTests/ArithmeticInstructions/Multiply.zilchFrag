struct Helper[Type]
{
  [Static]
  function Test()
  {
    var lhs = Type();
    var rhs = Type();
    var result = lhs * rhs;
    result *= rhs;
  }
}

[Pixel][UnitTest]
struct Multiply
{
  [EntryPoint]
  function Main()
  {
    Helper[Real].Test();
    Helper[Real2].Test();
    Helper[Real3].Test();
    Helper[Real4].Test();
    
    Helper[Integer].Test();
    Helper[Integer2].Test();
    Helper[Integer3].Test();
    Helper[Integer4].Test();
  }
}
