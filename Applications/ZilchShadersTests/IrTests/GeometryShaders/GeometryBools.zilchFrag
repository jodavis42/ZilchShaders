  [StageInput][StageOutput] var MyVar2 : Boolean2;
  [StageInput("MyVar2")][StageOutput("MyVar2")] var MyInt2 : Integer2;
  
  [StageInput][StageOutput] var MyVar3 : Boolean3;
  [StageInput("MyVar3")][StageOutput("MyVar3")] var MyInt3 : Integer3;
